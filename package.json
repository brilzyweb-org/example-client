{
  "name": "example-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"wrangler dev --persist-to ../../.wrangler/shared-d1\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --mode client",
    "build:server": "vite build --mode server",
    "deploy": "wrangler deploy",
    "deploy:all": "node scripts/deploy.js"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5",
    "postcss": "latest",
    "autoprefixer": "latest",
    "cssnano": "latest",
    "postcss-combine-media-query": "latest",
    "postcss-sort-media-queries": "latest",
    "purgecss": "latest",
    "rollup-plugin-critical": "latest",
    "vite": "^5.0.11",
    "wrangler": "^4.58.0"
  }
}